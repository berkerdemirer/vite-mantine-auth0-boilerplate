{
  "name": "vite-react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "downloadEn": "locize download --project-id=596dc0ad-dac9-48af-8088-6413160596b4 --language=en --ver=latest --clean=true --path=./src/@types/locales",
    "interface": "i18next-resources-for-ts interface -i ./src/@types/locales -o ./src/@types/resources.d.ts",
    "update-interface": "npm run downloadEn && npm run interface && rm -rf ./src/@types/locales"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.1",
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^7.0.0",
    "@mantine/dates": "^7.0.0",
    "@mantine/dropzone": "^7.0.0",
    "@mantine/hooks": "^7.0.0",
    "@mantine/modals": "^7.0.0",
    "@mantine/notifications": "^7.0.0",
    "@tabler/icons-react": "^2.34.0",
    "@tanstack/react-query": "^4.35.3",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-locize-backend": "^6.2.3",
    "locize-lastused": "^3.2.2",
    "react": "^18.2.0",
    "react-circle-flags": "^0.0.19",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-i18next": "^13.2.2",
    "react-router-dom": "^6.16.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^12.1.2",
    "i18next-resources-for-ts": "1.3.3",
    "locize-cli": "7.14.8",
    "postcss": "^8.4.30",
    "postcss-preset-mantine": "^1.7.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
